friends = ["Apple","Orange", 5, 345.056,"False","Akash","Rohan"]

print(friends[0])


friends[0]='grapes'   # unlike strings lists are mutable

print(friends[0])

print(friends[1:4])

friends.append("Aadi")

print(friends)


l1= [1,34,62,2,6,11]

l1.sort()
print(l1)


l2= [1,34,62,2,6,11]
l2.reverse()
print(l2)

l3= [1,34,62,2,6,11]
l3.insert(4,457)
print(l3)

l4= [1,34,62,2,6,11]
l4.pop(2)
print(l4)

l5= [1,34,62,2,6,11]
l5.remove(62)
print(l5)
